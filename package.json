{
  "name": "crop-image-data",
  "version": "1.0.2",
  "description": "Function for cropping an image through its ImageData.",
  "author": "Daniel Grefberg <hello@danielgrefberg.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duniul/crop-image-data.git"
  },
  "homepage": "https://github.com/duniul/crop-image-data#readme",
  "bugs": {
    "url": "https://github.com/duniul/crop-image-data/issues"
  },
  "keywords": [
    "image",
    "image-data",
    "image-editing",
    "canvas",
    "crop-image"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "rimraf lib && pnpm build:es && pnpm build:cjs",
    "build:es": "tsc --outDir lib/es --module es6",
    "build:cjs": "tsc --outDir lib/cjs --module commonjs",
    "dev": "rimraf lib && pnpm build:es --watch",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "lint": "biome check",
    "lint:apply": "biome check --apply",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.1",
    "@canvas/image-data": "1.0.0",
    "@changesets/cli": "^2.26.2",
    "changesets-changelog-clean": "1.2.1",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "typescript": "4.0.2",
    "vitest": "^0.34.6"
  }
}
